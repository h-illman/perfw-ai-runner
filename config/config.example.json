{
  "output_dir": "runs",
  "workload": "resnet50",
  "model": "resnet50",
  "duration_s": 60.0,
  "warmup_s": 10.0,
  "repeats": 3,
  "sample_hz": 2.0,
  "batch_size": 32,
  "precision": "fp16",
  "image_size": 224,
  "matmul_n": 2048,
  "allow_tf32": true,
  "notes": "",
  "influx": {
    "enabled": true,
    "url": "http://localhost:8181",
    "database": "perfw",
    "precision": "nanosecond",
    "accept_partial": true,
    "no_sync": true
  },
  "grafana": {
    "enabled": false,
    "dashboard_url": "http://localhost:3000",
    "open_on_start": true
  },
  "regression": {
    "enabled": true,
    "warn_drop_pct": 5.0,
    "fail_drop_pct": 10.0,
    "baseline_policy": "latest_pass"
  }
}